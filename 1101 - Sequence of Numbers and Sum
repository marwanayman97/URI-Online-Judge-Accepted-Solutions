#include <iostream>


using namespace std;

int main(){
 
 	 int M,N,sum=0;
 	 
 	 cin>> M >> N;
 	 
 	 while(M>0 && N>0){
	  
 	 
	   if (M<N){
	  	for(int i=M;i<=N;i++){
	  		sum +=i;
	  		cout << i << " ";
		  }
		  cout<< "Sum=" << sum << endl;
		  sum=0;
	  } else {
	  	
	  		for(int j=N;j<=M;j++){
	  		sum +=j;
	  		cout << j << " ";
		  }
		  cout<< "Sum=" << sum << endl;
		  sum=0;
	  }	 
 	 
 	 cin>> M >> N;
 }
 	
            
	  
  
   
 
  return 0;
  }
