#include <iostream>
#include <iomanip>

using namespace std;

int main(){
 
 	 double x,avg,sum=0;
 	 int cnt;
 	 
 	 cout<< fixed << setprecision(1);
 	 
 	 for(int i=1;i<=6;i++){
 	 	cin>>x;
 	 	if(x>=0){
		  
 	 	cnt++;
 	 	sum += x;}
	  }
	  
   cout << cnt << " valores positivos" << endl;
   cout << sum / cnt << endl;
 
  return 0;
  }
