#include <iostream>
#include <math.h>


using namespace std;

int main(){
           
    int ct_100,ct_50,ct_20,ct_10,ct_5,ct_2,ct_1,N,x;
    
    cin>>N;
    x=N;
    ct_100 = N/100;
    N %= 100 ;
    ct_50 = N/50;
    N %= 50;
    ct_20 = N/20;
    N %= 20;
    ct_10 = N/10;
    N %= 10;
    ct_5 = N/5;
    N %= 5;
    ct_2 = N/2;
    N %= 2;
    ct_1 = N;
    
    
    cout<< x <<endl;
    
    cout<< ct_100 << " nota(s) de R$ 100,00"<<endl;
    cout<< ct_50 << " nota(s) de R$ 50,00"<<endl;
    cout<< ct_20 << " nota(s) de R$ 20,00"<<endl;
    cout<< ct_10 << " nota(s) de R$ 10,00"<<endl;
    cout<< ct_5 << " nota(s) de R$ 5,00"<<endl;
    cout<< ct_2 << " nota(s) de R$ 2,00"<<endl;
    cout<< ct_1 << " nota(s) de R$ 1,00"<<endl;
    
		   
      
    return 0;
  }
