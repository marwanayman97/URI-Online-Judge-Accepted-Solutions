#include <iostream>
#include <iomanip>
using namespace std;

int main(){
 
  int number,hours;
  double amount;
  
  cin>> number >> hours >> amount;
  
  cout << fixed << setprecision(2) ;
  
  cout << "NUMBER = " << number << endl;
  cout << "SALARY = U$ " << hours * amount << endl;



return 0;
}
