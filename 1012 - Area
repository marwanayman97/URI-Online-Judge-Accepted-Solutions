#include <iostream>
#include <iomanip>
using namespace std;

int main(){
 
  double A,B,C,pi=3.14159;
  
  cout << fixed << setprecision(3) ;
  
  cin >> A >> B >> C;
  
  cout << "TRIANGULO: " << 0.5 * A * C << endl;
  
  cout << "CIRCULO: " << pi * C * C << endl;
  
  cout << "TRAPEZIO: " << C * ((A+B)/2) << endl;
  
  cout << "QUADRADO: " << B * B << endl;
  
  cout << "RETANGULO: " << A * B << endl;


return 0;
}
