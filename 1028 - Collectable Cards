#include <iostream>



using namespace std;

int main(){
	
	int N,n1,n2;
    

    
    cin>>N;
    
    for(int k=0;k<N;k++){

    	cin>>n1>>n2;
    	while(n1 != n2)
    {
        if(n1 > n2)
            n1 -= n2;
        else
            n2 -= n1;
    }
    	cout<<n1<<endl;
	}
    
   
    
	return 0;
}
