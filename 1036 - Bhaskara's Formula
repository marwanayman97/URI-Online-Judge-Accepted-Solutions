#include <iostream>
#include <iomanip>
#include <math.h>
using namespace std;

int main(){
 
  double A,B,C,R1,R2;
  
  cout << fixed << setprecision(5) ;
  
  cin >> A >> B >> C;
  
  R1 = (-1*B + sqrt((B*B) - (4*A*C))) / (2*A);
  R2 = (-1*B - sqrt((B*B) - (4*A*C))) / (2*A);
  if (A==0 || ((B*B) - (4*A*C)) < 0){
  	
  	cout << "Impossivel calcular" <<endl;
  	
  }else{

  cout << "R1 = " << R1 << endl;
  cout << "R2 = " << R2 << endl;
     }
  return 0;
  }
